

  node() {

  currentBuild.result = "SUCCESS"

  try {
      currentBuild.description = "my new description"

      stage('Checkout') {
        checkout scm
      }
      stage('NuGet') {
        timestamps {
           bat 'exitcodes.bat'
          }
     }
    stage('WrapUp') {
        timestamps {
            bat 'echo "Success"'
            sleep(1);
          }
     }
    }
    catch  (err) {
      println(err);
      currentBuild.result = "FAILURE"
      bat 'echo failure'
    }
  }
